{
  "name": "atomic-spinner",
  "version": "1.0.17",
  "description": "Loading spinner that looks like an atom",
  "author": "David Lounsbrough",
  "license": "MIT",
  "repository": "Codenough-LLC/atomic-spinner",
  "main": "dist/index",
  "typings": "dist/index",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "publish-npm": "pnpm clean && pnpm build && npm publish",
    "build-demo": "pnpm --prefix demo build",
    "deploy-demo": "pnpm build-demo && gh-pages -d demo/build"
  },
  "keywords": [
    "atomic",
    "atom",
    "chemistry",
    "loader",
    "loading",
    "spinner",
    "npm",
    "package",
    "react",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^2.9.2"
  }
}
